   "source": [
    "import numpy as np\n",
    "import msicpe.ssl as ssl\n",
    "\n",
    "Fe = 96000      #Fréqence d'échantillonage\n",
    "Fc = 3000       #Fréquence de coupure pour les filtres passe-bas\n",
    "\n",
    "def f_passebas(S):\n",
